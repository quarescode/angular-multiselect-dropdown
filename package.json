{
  "name": "@khajegan/angular2-multiselect-dropdown",
  "version": "17.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-sass": "node-sass --include-path scss projects/angular2-multiselect-dropdown-lib/themes/default.theme.scss projects/angular2-multiselect-dropdown-lib/themes/default.theme.css",
    "build-package": "npm run build-sass && ng-packagr -p projects/angular2-multiselect-dropdown-lib/package.json",
    "github-pages": "ng build --prod --base-href /angular2-multiselect-dropdown/",
    "myScope": "cd projects/angular2-multiselect-dropdown-lib && sudo npm init --scope=@khajegan",
    "myBuild": "cd projects/angular2-multiselect-dropdown-lib && sudo ng b --configuration production &&  sudo cp -r themes ../../dist/angular2-multiselect-dropdown-lib/",
    "myPublish": "cd dist/angular2-multiselect-dropdown-lib && npm publish --access public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.7",
